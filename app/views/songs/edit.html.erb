<%= form_for @song do |f| %>
  <div class="field">
    <%= f.label "Song title" %>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label "is released" %>
    <%= f.check_box :released %>
  </div>
  <div class="field">
    <%= f.label "Release Year" %>
    <%= f.text_field :release_year %>
  </div>
  <div class="field">
    <%= f.label "Artist Name" %>
    <%= f.text_field :artist_name %>
  </div>
  <div class="field">
    <%= f.label "Genre" %>
    <%= f.text_field :genre %>
  </div>
  <div class="field">
    <%= f.submit %>
  </div>
<% end %>
